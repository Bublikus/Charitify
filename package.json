{
  "name": "charitify",
  "description": "Charity app for helping those in need.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy --basepath Charitify",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "deploy": "npm run export && gh-pages -d __sapper__/export/Charitify && git add . && git commit -m 'deploy' && git push"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "compression": "^1.7.1",
    "dayjs": "^1.8.23",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.1",
    "js-storage": "^1.1.0",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "polka": "^1.0.0-next.11",
    "sirv": "^0.4.0",
    "zl-fetch": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.8.4",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^6.1.0",
    "@rollup/plugin-replace": "^2.3.1",
    "autoprefixer": "^9.7.4",
    "babel-plugin-lodash": "^3.3.4",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-visualizer": "^3.3.2",
    "sapper": "^0.27.0",
    "sapper-environment": "^1.0.1",
    "svelte": "^3.18.1",
    "svelte-preprocess": "^3.5.0"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Bublikus/Charitify.git"
  },
  "license": "SEE LICENSE IN README.md"
}
